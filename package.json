{
  "name": "bun-dev",
  "version": "0.1.1",
  "author": {
    "name": "xpriori"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run ./src/index.tsx --watch",
    "zig:tspg": "zig build-lib ./src/ffi/ts_to_pg.zig -dynamic -OReleaseFast",
    "zig:ots": "zig build-lib ./src/ffi/obj_to_ts.zig -dynamic -OReleaseFast",
    "lint": "bun run lint.mjs",
    "lint:fix": "bun run lint.mjs --fix",
    "build": "bunchee src/index.tsx"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/bun": "^1.2.13",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "bunchee": "^6.5.2",
    "eslint": "^9.26.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4"
  },
  "peerDependencies": {
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "bun": "^1.2.13",
    "chalk": "^5.4.1",
    "ora": "^8.2.0",
    "ts-to-zod": "^3.15.0"
  }
}
